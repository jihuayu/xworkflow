{
  "version": "0.1.0",
  "nodes": [
    {
      "id": "start",
      "data": {
        "type": "start",
        "title": "Start",
        "variables": [
          { "variable": "first_name", "label": "First", "type": "string", "required": true },
          { "variable": "last_name", "label": "Last", "type": "string", "required": true },
          { "variable": "age", "label": "Age", "type": "number", "required": true }
        ]
      }
    },
    {
      "id": "tmpl",
      "data": {
        "type": "template-transform",
        "title": "Format",
        "template": "Name: {{ first }} {{ last | upper }}, Age: {{ age }}",
        "variables": [
          { "variable": "first", "value_selector": ["start", "first_name"] },
          { "variable": "last", "value_selector": ["start", "last_name"] },
          { "variable": "age", "value_selector": ["start", "age"] }
        ]
      }
    },
    {
      "id": "end",
      "data": {
        "type": "end",
        "title": "End",
        "outputs": [
          { "variable": "result", "value_selector": ["tmpl", "output"] }
        ]
      }
    }
  ],
  "edges": [
    { "source": "start", "target": "tmpl" },
    { "source": "tmpl", "target": "end" }
  ]
}
